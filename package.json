{
  "name": "d2-nutrition-vibes",
  "version": "1.0.0",
  "description": "D2 Nutrition Vibes - Agricultural data visualization application",
  "type": "module",
  "main": "src/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:analyze": "vue-tsc && vite build --mode analyze",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.vue,.ts --fix",
    "format": "prettier --write src/**/*.{js,vue,ts}",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "cypress run",
    "e2e:dev": "cypress open",
    "bundle-analyze": "npx vite-bundle-analyzer dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@vee-validate/zod": "^4.15.1",

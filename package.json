    "cypress-axe": "^1.6.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-vue": "^9.18.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.0",
    "prettier": "^3.1.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "storybook": "^8.6.14",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-bundle-analyzer": "^0.11.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^1.8.0"

    "d3": "^7.8.5",
    "pinia": "^2.1.0",
    "terser": "^5.43.1",
    "topojson-client": "^3.1.0",
    "vee-validate": "^4.15.1",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^6.0.3",
    "@percy/cypress": "^3.1.6",
    "@storybook/addon-a11y": "^8.6.14",
